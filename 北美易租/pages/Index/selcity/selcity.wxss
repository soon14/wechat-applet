
page{
  height: 100%;
  border-top:1rpx solid #DFDFDF;
}
.city-tabcon .tabcon-item{
  width:50%;
  float: left;
}
.city-tabcon .selcity-tablist{
  margin:0 auto;
  margin-bottom:30rpx;
  font-size: 32rpx;
  width:152rpx;
  height:64rpx;
  line-height: 64rpx;
  text-align: center; 
}
.search-textcon .serach-icon{
  position:absolute;
  left:16rpx;
  top:13rpx;
  width:34rpx;
  height:32rpx;
}

.show-content{
  display: block;
  margin-bottom:50rpx;
}
.hidden-content{
  display: none;
}
.show-tab{
  color:#f47a20;
  border-bottom:4rpx solid #f47a20;
}
.scroll-container,.search-item,.scroll-view{
  width: 100%;
  height: 100%;
}

.letterlist{
  position: fixed;
  top: 130rpx;
  right: 0;
  width: 52rpx;
  z-index: 9;
  text-align: center;
}
.letterlist .letter{
  width: 52rpx;
  height: 40rpx;
  line-height: 40rpx;
} 


.list-typelist{
  margin:0 42rpx;
  text-align: left;
  height:76rpx;
  line-height: 76rpx;
  border-bottom:4rpx solid #dfdfdf;
}
.list-typename{
  padding-left:42rpx;
  margin-top: 20rpx;
  background:#f6f6f6;
  width:100%;
  text-align: left;
  height:74rpx;
  line-height: 74rpx;
}
.list-typelist .text-state{
  color:#dfdfdf;
}
