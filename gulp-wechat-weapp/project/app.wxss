@charset "UTF-8";
page,
view,
text {
  box-sizing: border-box; }

view {
  font-family: Helvetica, Tahoma, "Microsoft YaHei", "PingFang SC", sans-serif; }

@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format("truetype");
  font-weight: 500;
  font-style: normal; }

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px; }

.icon-lock:before {
  content: "\e620"; }

.icon-spinner:before {
  content: "\e614"; }

.icon-delete:before {
  content: "\e6a8"; }

.icon-star:before {
  content: "\e610"; }

.icon-sound:before {
  content: "\e62e"; }

.icon-voice:before {
  content: "\e611"; }

.icon-rec:before {
  content: "\e773"; }

.icon-play:before {
  content: "\e606"; }

.icon-pause:before {
  content: "\e60a"; }

.icon-arrow-left:before {
  content: "\e65a"; }

.icon-arrow-right:before {
  content: "\e65b"; }

/*** 弹性布局 ***/
.flex {
  display: -webkit-flex;
  display: -webkit-box;
  display: flex; }

.flex-x {
  display: -webkit-flex;
  display: -webkit-box;
  display: flex;
  -webkit-justify-content: center;
  -webkit-box-pack: center;
          justify-content: center; }

.flex-y {
  display: -webkit-flex;
  display: -webkit-box;
  display: flex;
  -webkit-align-items: center;
  -webkit-box-align: center;
          align-items: center; }

.flex-xy {
  display: -webkit-flex;
  display: -webkit-box;
  display: flex;
  -webkit-justify-content: center;
  -webkit-box-pack: center;
          justify-content: center;
  -webkit-align-items: center;
  -webkit-box-align: center;
          align-items: center;
  position: relative; }

.container {
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box; }
